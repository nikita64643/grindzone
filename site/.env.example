APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Moneta | PayAnyWay — пополнение баланса
# В настройках счёта Moneta укажите PayURL: {APP_URL}/payment/webhook
# MONETA_ACCOUNT_ID=
# MONETA_SECRET=
# MONETA_DEMO_MODE=true
# MONETA_USERNAME=
# MONETA_PASSWORD=
# MONETA_PAYMENT_PASSWORD=
# MONETA_LOCALE=ru
# MONETA_CURRENCY=RUB
# MONETA_UNIT_ID=
# MONETA_LIMIT_IDS=

# EasyDonate — альтернативная оплата привилегий (https://docs.easydonate.ru/)
# server_id и product_id задаются в БД (minecraft_servers, privileges)
# Callback URL в панели EasyDonate: {EASYDONATE_PUBLIC_URL}/payment/webhook-easydonate
# EASYDONATE_ENABLED=false
# EASYDONATE_SHOP_KEY=
# EASYDONATE_PUBLIC_URL=  — публичный URL (обязательно при 192.168.x.x). Пример: https://minecruft.ru

# OAuth — привязка и вход через соц. сети (настройки → Привязка соц. сетей)
# VK: https://vk.com/editapp?act=create
# VKONTAKTE_CLIENT_ID=
# VKONTAKTE_CLIENT_SECRET=
# VKONTAKTE_REDIRECT_URI=${APP_URL}/auth/vkontakte/callback
# Discord: https://discord.com/developers/applications
# DISCORD_CLIENT_ID=
# DISCORD_CLIENT_SECRET=
# DISCORD_REDIRECT_URI=${APP_URL}/auth/discord/callback
# Telegram: @BotFather → /newbot, /setdomain
# TELEGRAM_BOT_NAME=
# TELEGRAM_TOKEN=
# TELEGRAM_REDIRECT_URI=${APP_URL}/auth/telegram/callback
# Google: https://console.cloud.google.com/apis/credentials
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=
# GOOGLE_REDIRECT_URI=${APP_URL}/auth/google/callback
# Max (мессенджер VK): OAuth — требуется кастомная реализация
# MAX_CLIENT_ID=
# MAX_CLIENT_SECRET=
# MAX_REDIRECT_URI=${APP_URL}/auth/max/callback

# Donate → LuckPerms (RCON). Для выдачи групп после доната на серверах 1.21.
# DONATE_LUCKPERMS_SYNC=true
# MINECRAFT_RCON_HOST=127.0.0.1
# MINECRAFT_RCON_PASSWORD=
# MINECRAFT_RCON_TIMEOUT=5
# MINECRAFT_RCON_PORT_OFFSET=10000
